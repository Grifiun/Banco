/Agregar intervalo dinero
    //RESPUESTA
    SELECT A.* FROM TRANSACCION A JOIN CUENTA B ON A.cuenta_id = B.codigo WHERE (B.cliente = "CLI4860") AND (monto BETWEEN "2500" AND "5000") ORDER BY A.fecha DESC, A.hora, A.monto DESC;
